{
  "name": "mobile-vuecli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf \"./dist\"",
    "serve": "npm run clean && vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:www": "cross-env OUTPUT_DIR=www npm run build",
    "serve:prod": "cross-env NODE_ENV=production npm run serve",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ant-design-vue": "^1.3.10",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-exists": "^5.0.1",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.9.0",
    "postcss-pxtorem-each": "^0.0.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
